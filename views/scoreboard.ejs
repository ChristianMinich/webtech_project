<!DOCTYPE html>
<html>
<head>
  <title>User Highscores</title>
  <style>
    table {
      border-collapse: collapse;
    }
    th, td {
      border: 1px solid black;
      padding: 8px;
    }
  </style>
</head>
<body>
  <%- include('navbar') %>
  <h1>User Highscores</h1>
  <table>
    <thead>
      <tr>
        <th>Avatar</th>
        <th>Username</th>
        <th>Highscore</th>
      </tr>
    </thead>
    <tbody>
      <% rows.forEach(function(row) { %>
        <tr>
            <td><img src="/assets/<%= row.FILE_PATH%>" width="20px" height="20px"></td>
            <td><a href="/profile/<%= row.USERNAME %>"><%= row.USERNAME %></a></td>
          <td><%= row.HIGHSCORE %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>
